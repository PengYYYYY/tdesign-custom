{
  "name": "tdesign-custom",
  "version": "0.0.1",
  "author": "py",
  "type": "module",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "esm",
    "LICENSE",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "init": "git submodule init && git submodule update && cd _tdesign-vue && git submodule init && git submodule update",
    "dev:site": "cd site/ && vite",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "prebuild": "rimraf es/* esm/* lib/* cjs/* dist/*",
    "preview": "cd site && vite preview",
    "build:site": "cd site/ && vite build",
    "build:pkg": "npm run prebuild && cross-env NODE_ENV=production rollup -c scripts/rollup.config.js && npm run build:tsc",
    "build:tsc": "run-p build:tsc-*",
    "build:tsc-es": "tsc --emitDeclarationOnly -d --outDir es/",
    "build:tsc-esm": "tsc --emitDeclarationOnly -d --outDir esm/",
    "test": "npm run test:unit && npm run test:snap",
    "test:unit": "vitest run --config test/vitest.config.js",
    "test:update": "vitest run --config test/vitest.config.js -u && npm run test:snap-update",
    "test:unit-dev": "vitest --config test/vitest.config.js",
    "test:unit-gui": "vitest --config test/vitest.config.js --ui",
    "test:snap": "cross-env NODE_ENV=test-snap vitest run --config test/vitest.config.js",
    "test:snap-update": "cross-env NODE_ENV=test-snap vitest run --config test/vitest.config.js -u"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "~7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.5.5",
    "@rollup/plugin-babel": "^6.0.2",
    "@rollup/plugin-commonjs": "^23.0.2",
    "@rollup/plugin-json": "^5.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-url": "^8.0.1",
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.4",
    "@types/rimraf": "^3.0.2",
    "@vitest/coverage-c8": "^0.25.2",
    "@vitest/ui": "^0.25.2",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/compiler-dom": "^3.2.41",
    "@vue/composition-api": "^1.7.1",
    "@vue/test-utils": "1.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-polyfill": "^6.26.0",
    "camelcase": "^7.0.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "esno": "^0.16.3",
    "execa": "^6.1.0",
    "glob": "^8.0.3",
    "gray-matter": "^4.0.3",
    "jsdom": "^20.0.2",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.29.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-multi-input": "^1.1.1",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-styles": "^4.0.0",
    "rollup-plugin-terser": "~7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "rollup-pluginutils": "^2.8.2",
    "tdesign-icons-view": "^0.1.5",
    "tdesign-icons-vue": "^0.1.4",
    "tdesign-site-components": "^0.11.5",
    "tdesign-theme-generator": "^0.5.0",
    "tdesign-vue": "0.49.3",
    "typescript": "^4.8.4",
    "unplugin-vue2-script-setup": "^0.11.3",
    "vite": "^3.1.8",
    "vite-plugin-pwa": "^0.13.2",
    "vite-plugin-tdoc": "^2.0.4",
    "vite-plugin-vue2": "^2.0.2",
    "vitest": "^0.25.2",
    "vitest-fetch-mock": "^0.2.1",
    "vue": "2.6.14",
